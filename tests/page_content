var foot_visible = 0;
var cmpcall = 0;
jQuery(window).scroll(function(){ 
	if(cmpcall == 0){
		cmpcall = 1;
		pass_logurl("comp_page_scroll");  
	}
	var scrollTop = jQuery(window).scrollTop();    
	if((scrollTop > 750) && (scrollTop < 1500))
	{	
		if(foot_visible == 0){
			foot_visible = 1;
			pass_logurl("vis_foot");		
		}
	}
	else if(scrollTop == 0)
	{
		cmpcall = 0;
		foot_visible = 0;
	}  
});
