<script type="text/javascript">
function timenow() {
	t = (((new Date).getTime())*1000)+Math.round(Math.random()*1000);
	var querystring = "";
	var tmp_ref = encodeURIComponent(document.referrer);		
	var tmp_url = document.location.href ;	
	var curr_url = tmp_url.split("?");		
	if(curr_url[1]){querystring = curr_url[1];}    	
	if((tmp_ref == null) || (tmp_ref.length == 0)){tmp_ref = "";}	
	if(tmp_ref != ""){t += "&ref="+tmp_ref;}	
	if(querystring != "" ){t += "&"+querystring;}
	
			var url11 = "http://metric.money.rediff.com/frames/ratio.htm?companyCode=15130025&"+'done='+t;
		
	if(document.getElementById("logwatch")){document.getElementById("logwatch").src = url11;}
}
</script>


